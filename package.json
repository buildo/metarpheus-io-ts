{
  "name": "metarpheus-io-ts",
  "version": "0.6.0",
  "description": "metarpheus to io-ts",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "prettier-check": "prettier --list-different \"{src,test}/**/*.ts\"",
    "test": "npm run lint && npm run prettier-check && jest && npm run typecheck-output",
    "clean": "rm -rf lib/*",
    "build": "npm run clean && tsc",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "release-version": "smooth-release",
    "typecheck-output": "ts-node output && tsc --project output/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/metarpheus-io-ts.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/metarpheus-io-ts/issues"
  },
  "homepage": "https://github.com/gcanti/metarpheus-io-ts",
  "dependencies": {
    "commander": "^2.9.0",
    "fp-ts": "^1.12.0",
    "io-ts-codegen": "^0.2.2",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.76",
    "@types/node": "7.0.4",
    "axios": "^0.17.1",
    "io-ts": "^1.0.5",
    "jest": "^23.6.0",
    "prettier": "^1.11.1",
    "smooth-release": "^8.0.4",
    "ts-jest": "^23.10.5",
    "ts-node": "^5.0.1",
    "tslint": "4.4.2",
    "tslint-config-standard": "4.0.0",
    "typescript": "^3.2.0"
  },
  "tags": [],
  "keywords": [],
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
