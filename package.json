{
  "name": "metarpheus-io-ts",
  "version": "0.3.0",
  "description": "metarpheus to io-ts",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "prettier-check": "prettier --list-different \"{src,test}/**/*.ts\"",
    "mocha": "mocha -r ts-node/register test/*.ts",
    "test": "npm run lint && npm run prettier-check && npm run mocha && npm run typecheck-output",
    "clean": "rm -rf lib/*",
    "build": "npm run clean && tsc",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "release-version": "smooth-release",
    "typecheck-output": "ts-node output && tsc --project output/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/metarpheus-io-ts.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/metarpheus-io-ts/issues"
  },
  "homepage": "https://github.com/gcanti/metarpheus-io-ts",
  "dependencies": {
    "commander": "^2.9.0",
    "fp-ts": "^1.7.0",
    "io-ts-codegen": "^0.1.8",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.76",
    "@types/mocha": "2.2.38",
    "@types/node": "7.0.4",
    "axios": "^0.17.1",
    "io-ts": "^1.0.5",
    "mocha": "3.2.0",
    "prettier": "^1.11.1",
    "smooth-release": "^8.0.4",
    "ts-node": "^5.0.1",
    "tslint": "4.4.2",
    "tslint-config-standard": "4.0.0",
    "typescript": "^2.8.1"
  },
  "tags": [],
  "keywords": []
}
